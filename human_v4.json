{
  "name": "human cerebellum cortex",
  "storage": {
    "engine": "hdf5",
    "root": "HumanMorphologies.hdf5"
  },
  "network": {
    "x": 400.0,
    "y": 600.0,
    "z": 400.0
  },
  "regions": {
    "cerebellar_cortex": {
      "type": "stack",
      "origin":[0.0, 0.0, 0.0],
      "partitions":["granular_layer", "purkinje_layer", "upper_molecular_layer","lower_molecular_layer"]
    }
  },
  "partitions": {
    "granular_layer": {
      "region":"cerebellar_cortex",
      "type":"layer",
      "thickness": 260.0
    },
    "purkinje_layer": {
      "region":"cerebellar_cortex",
      "type":"layer",
      "thickness": 30.0
    },
    "upper_molecular_layer": {
      "region":"cerebellar_cortex",
      "type":"layer",
      "thickness": 200.0
    },
    "lower_molecular_layer":{
      "region":"cerebellar_cortex",
      "type":"layer",
      "thickness": 100.0
    }      
  },
  "cell_types": {
    "granule_cell": {
      "spatial": {
        "radius": 2.4,
        "density": 2.5e-3
      },
      "geometry": {
        "class": "bsb.morphologies.NoGeometry",
        "pf_height": 0,
        "pf_height_sd": 0,
        "pf_length": 0,
        "pf_radius": 0,
        "dendrite_length": 40
      },
      "morphologies": [
        {
          "names": ["GranuleCell"]
        }
      ],
      "plotting": {
        "display_name": "granule cell",
        "color": "#E62214",
        "opacity": 0.3
      }
    },
    "glomerulus": {
      "spatial": {
        "radius": 1.5,
        "density": 3e-4
      },
      "morphology": {
        "class": "bsb.morphologies.NoGeometry"
      },
      "plotting": {
        "display_name": "glomerulus",
        "color": "#0E1030",
        "opacity": 0.3
      }
    },
    "golgi_cell": {
      "spatial": {
        "radius": 8.0,
        "density": 2.2e-6
      },
      "morphology": {
        "class": "bsb.morphologies.GolgiCellGeometry",
        "dendrite_radius": 120.0,
        "axon_x": 400.0,
        "axon_y": 400.0,
        "axon_z": 30.0,
        "detailed_morphologies": {
          "names": ["GolgiCell"]
        }
      },
      "plotting": {
        "display_name": "golgi cell",
        "color": "#332EBC",
        "opacity":0.3
      }
    },
    "purkinje_cell": {
      "spatial": {
        "radius": 15.0,
        "planar_density": 0.000180,
        "extension_x": 350.0,
        "extension_z": 3.5,
        "angle": 70
      },
      "morphology": {
        "class": "bsb.morphologies.NoGeometry",
        "detailed_morphologies": ["PurkinjeCell"]
      },
      "plotting": {
        "display_name": "Purkinje cell",
        "color": "#0F8944",
        "opacity":0.3
      }
    },
    "stellate_cell": {
      "spatial": {
        "radius": 5.0,
        "density": 0.2e-4,
        "y_restriction": [0.33, 1.00]
      },
      "morphology": {
        "class": "bsb.morphologies.NoGeometry",
        "detailed_morphologies": ["StellateCell"]
      },
      "plotting": {
        "display_name": "Stellate cell",
        "color": "#FFC425",
        "opacity":0.3
      }
    },
    "basket_cell": {
      "spatial": {
        "radius": 6.0,
        "density": 0.2e-4,
        "y_restriction": [0.00, 0.33]
      },
      "morphology": {
        "class": "bsb.morphologies.NoGeometry",
        "detailed_morphologies": ["BasketCell"]
      },
      "plotting": {
        "display_name": "Basket cell",
        "color": "#F37735"
      }
    },
    "mossy_fibers": {
      "entity": true,
      "relay": true,
      "spatial": {
        "density":1.5e-5
      },
      "plotting": {
        "display_name": "mossy_fibers",
        "color": "black"
      }
    }
  },
  "placement":{
    "granule_cell_placement": {
      "cls": "bsb.placement.ParticlePlacement",
      "cell_types": ["granule_cell"],
      "partitions": ["granular_layer"]
    },
    "glomerulus_placement": {
      "cls": "bsb.placement.ParticlePlacement",
      "cell_types": ["glomerulus"],
      "partitions": ["granular_layer"]
    },
    "purkinje_placement": {
      "cls": "bsb.placement.ParallelArrayPlacement",
      "cell_types": ["purkinje_cell"],
      "spacing_x": 350.0,
      "spacing_z": 3.5,
      "angle" : 70,
      "partitions": ["purkinje_layer"]
    },
    "golgi_cell_placement": {
      "cls": "bsb.placement.ParticlePlacement",
      "cell_types": ["golgi_cell"],
      "partitions": ["granular_layer"]
    },
    "stellate_cell_placement": {
      "cls": "bsb.placement.ParticlePlacement",
      "cell_types": ["stellate_cell"],
      "partitions": ["upper_molecular_layer","lower_molecular_layer"]
    },
    "basket_cell_placement": {
      "cls": "bsb.placement.ParticlePlacement",
      "cell_types": ["basket_cell"],
      "partitions": ["upper_molecular_layer","lower_molecular_layer"]
    },
    "mossy_fibers_placement": {
      "cls": "bsb.placement.Entities",
      "cell_types":["mossy_fibers"],
      "layer": "granular_layer",
      "partitions": ["granular_layer"],
      "placement_relative_to": "glomerulus",
      "placement_count_ratio": 0.05
    }
  },
  "after_placement": {
    "microzones": {
      "cls": "bsb.postprocessing.LabelMicrozones",
      "targets": ["purkinje_cell"]
    }
  },
  "connectivity": {
  "mossy_to_glomerulus":{
    "cls": "cerebellum.connectome.MossyGlomerulus",
    "presynaptic": {
      "cell_types": ["mossy_fibers"]
    },
    "postsynaptic": {
      "cell_types": ["glomerulus"],
      "compartments":["soma"]
    }
  },
  "glomerulus_to_granule": {
    "cls": "cerebellum.connectome.GlomerulusGranule",
    "after": ["mossy_to_glomerulus"],
    "presynaptic": {
      "cell_types": ["glomerulus"],
      "compartments":["soma"]
    },
    "postsynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["dendrites"]
    },
    "convergence": 4,
    "detailed": true
  },
  "golgi_to_glomerulus": {
    "cls": "cerebellum.connectome.GolgiGlomerulus",
    "presynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments":["soma"]
    },
    "postsynaptic": {
      "cell_types": ["glomerulus"],
      "compartments": ["soma"]
    },
    "divergence": 40,
    "detailed": true

  },
  "glomerulus_to_golgi": {
    "cls": "cerebellum.connectome.GlomerulusGolgi",
    "presynaptic": {
      "cell_types": ["glomerulus"],
      "compartments":["soma"]
    },
    "postsynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments": ["basal_dendrites"]
    },
    "detailed": true
  },
  "golgi_to_granule": {
    "cls": "cerebellum.connectome.GolgiGranule",
    "after": ["golgi_to_glomerulus", "glomerulus_to_granule"],
    "presynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments":["axon"]
    },
    "postsynaptic": {
      "cell_types": ["glomerulus"],
      "compartments": ["dendrites"]
    },
    "detailed": true
  },
  "golgi_to_golgi": {
    "cls": "bsb.connectivity.VoxelIntersection",
    "presynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments": ["axon"]
    },
    "postsynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments": ["basal_dendrites"]
    },
    "affinity": 0.5,
    "contacts": {
      "distribution": "norm",
      "loc": 160,
      "scale": 5
    }
  },
  "gap_goc": {
    "cls": "bsb.connectivity.VoxelIntersection",
    "presynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments":["basal_dendrites"]
    },
    "postsynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments": ["basal_dendrites"]
    },
    "affinity": 0.2,
    "contacts": {
      "distribution": "norm",
      "loc": 3,
      "scale": 1
    }
  },
  "ascending_axon_to_golgi": {
    "cls": "bsb.connectivity.TouchDetector",
    "presynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["ascending_axon"]
    },
    "postsynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments": ["basal_dendrites"]
    },
    "compartment_intersection_radius": 3.0
  },
  "parallel_fiber_to_golgi": {
    "cls": "bsb.connectivity.FiberIntersection",
    "presynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["parallel_fiber"]
    },
    "postsynaptic": {
      "cell_types": ["golgi_cell"],
      "compartments": ["apical_dendrites"]
    },
    "resolution": 20.0,
    "affinity": 0.1
  },
  "ascending_axon_to_purkinje": {
    "cls": "bsb.connectivity.TouchDetector",
    "presynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["ascending_axon"]
    },
    "postsynaptic": {
      "cell_types": ["purkinje_cell"],
      "compartments": ["aa_targets"]
    },
    "compartment_intersection_radius": 3.0,
    "synapses": {
      "type": "norm",
      "loc": 4,
      "scale": 0.4
    }
  },
  "parallel_fiber_to_purkinje": {
    "cls": "cerebellum.connectome.PFPurkinje",
    "presynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["parallel_fiber"]
    },
    "postsynaptic": {
      "cell_types": ["purkinje_cell"],
      "compartments": ["pf_targets"]
    },
    "resolution": 20.0,
    "affinity": 0.1
  },
  "parallel_fiber_to_basket": {
    "cls": "bsb.connectivity.FiberIntersection",
    "after": ["golgi_to_granule"],
    "presynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["parallel_fiber"]
    },
    "postsynaptic": {
      "cell_types": ["basket_cell"],
      "compartments": ["dendrites"]
    },
    "resolution": 20.0,
    "affinity": 0.1
  },
  "parallel_fiber_to_stellate": {
    "cls": "bsb.connectivity.FiberIntersection",
    "after": ["golgi_to_granule"],
    "presynaptic": {
      "cell_types": ["granule_cell"],
      "compartments":["parallel_fiber"]
    },
    "postsynaptic": {
      "cell_types": ["stellate_cell"],
      "compartments": ["dendrites"]
    },
    "resolution": 20.0,
    "affinity": 0.1
  },
  "stellate_to_purkinje": {
    "cls": "cerebellum.connectome.BCSCPurkinje",
    "presynaptic": {
      "cell_types": ["stellate_cell"],
      "compartments":["axon"]
    },
    "postsynaptic": {
      "cell_types": ["purkinje_cell"],
      "compartments": ["sc_targets"]
    },
    "affinity": 0.1
  },
  "basket_to_purkinje": {
    "cls": "cerebellum.connectome.BCSCPurkinje",
    "presynaptic": {
      "cell_types": ["basket_cell"],
      "compartments":["axon"]
    },
    "postsynaptic": {
      "cell_types": ["purkinje_cell"],
      "compartments": ["soma"]
    },
    "voxels_post": 1
  },
  "stellate_to_stellate": {
    "cls": "bsb.connectivity.VoxelIntersection",
    "presynaptic": {
      "cell_types": ["stellate_cell"],
      "compartments":["axon"]
    },
    "postsynaptic": {
      "cell_types": ["stellate_cell"],
      "compartments": ["dendrites"]
    },
    "affinity": 0.5,
    "contacts": {
      "distribution": "norm",
      "loc": 100,
      "scale":4
    }
  },
  "basket_to_basket": {
    "cls": "bsb.connectivity.VoxelIntersection",
    "presynaptic": {
      "cell_types": ["basket_cell"],
      "compartments":["axon"]
    },
    "postsynaptic": {
      "cell_types": ["basket_cell"],
      "compartments": ["dendrites"]
    },
    "affinity": 0.5,
    "contacts": {
      "distribution": "norm",
      "loc": 100,
      "scale":4
    }
  }
},
  "after_connectivity": {
  },
  "simulations": {
  }
}