        "cell_types": ["golgi_cell"],
        "compartments": ["basal_dendrites"]
      },
      "affinity": 0.5,
      "contacts": {
        "type": "norm",
        "loc": 160,
        "scale": 5
      }
    },
    "gap_goc": {
